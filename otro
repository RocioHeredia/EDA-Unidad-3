def REP_visita(self, s, tiempo, d):
        tiempo = tiempo + 1
        d[s] = tiempo
        for u in self.adyacentes(s):
            if d[u] == 0:
                self.REP_visita(u, tiempo, d)
    def REP(self):
        d = np.empty(self.__vertices)
        for v in self.__vertices:
            d[v] = 0
        tiempo = 0
        for s in self.__vertices:
            if d[s] == 0:
                self.REP_visita(s, tiempo, d)
